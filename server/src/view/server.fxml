<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="490.0" prefWidth="609.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ServerController">
   <children>
      <TextArea fx:id="textArea" prefHeight="450.0" prefWidth="400.0" />
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="150.0" spacing="20.0">
         <children>
            <Button fx:id="btnAdd" alignment="CENTER" mnemonicParsing="false" prefHeight="81.0" prefWidth="182.0" text="이미지 추가하기">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <HBox prefHeight="27.0" prefWidth="149.0">
               <children>
                  <Label alignment="CENTER" prefHeight="33.0" prefWidth="79.0" text="접속자">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Button id="sizeS" fx:id="btnRef" mnemonicParsing="false" prefHeight="33.0" prefWidth="87.0" text="새로고침">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <TextField fx:id="txtFieldClients" prefHeight="21.0" prefWidth="149.0">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin></TextField>
            <Button fx:id="btnLunch" alignment="CENTER" mnemonicParsing="false" prefHeight="81.0" prefWidth="182.0" text="서버 시작하기">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="btnClose" alignment="CENTER" mnemonicParsing="false" prefHeight="81.0" prefWidth="182.0" text="프로그램 종료">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
